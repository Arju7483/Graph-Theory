#include<bits/stdc++.h>
using namespace std;
const int N = 1000;
vector<int>graph[N] , ans ;
int indegree[N];
int main(){
    int n , m;
    cin >> n >> m;
    while(m--){
        int u,v;
        cin >> u >> v;
        graph[u].push_back(v);
        indegree[v]++;
    }
    vector<int>zeroIndegree;
    for(int i=1;i<=n;i++){
        if(indegree[i] == 0){
            zeroIndegree.push_back(i);
        }
    }
    for(int i=0;i<zeroIndegree.size();i++){
        ans.push_back(zeroIndegree[i]);
        for(auto node:graph[zeroIndegree[i]]){
            indegree[node]--;
            if(indegree[node]==0)zeroIndegree.push_back(node);
        }
    }
    if(ans.size()!=n){
        cout<<"Can not be sort"<<endl;
    }
    else{
        for(auto i:ans)cout<<i<<" ";
    }
}
